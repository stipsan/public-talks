{
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stipsan/public-talks.git"
  },
  "main": "server.js",
  "scripts": {
    "prebuild": "rimraf 'public/**/*.js' 'public/**/*.css' 'public/**/*.map'",
    "build": "NODE_ENV=production rollup -c && NODE_ENV=production yarn postcss",
    "precommit": "lint-staged",
    "postcss": "postcss client.css -o public/client.css",
    "predev": "yarn prebuild",
    "dev": "NODE_ENV=development concurrently 'micro-dev' 'rollup -c --watch' 'yarn postcss --watch' -k",
    "start": "NODE_ENV=production micro"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@reach/router": "^1.1.1",
    "accepts": "^1.3.5",
    "browserslist-useragent": "^2.0.1",
    "caniuse-api": "^3.0.0",
    "classnames": "^2.2.6",
    "intersection-observer": "^0.5.0",
    "micro": "^9.3.3",
    "path-to-regexp": "^2.4.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-transition-group": "^2.4.0",
    "serve-handler": "^5.0.2",
    "styled-components": "4.0.0-beta.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.0.1",
    "cssnano": "^4.1.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-unicorn": "^6.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "micro-dev": "^3.0.0",
    "postcss-cli": "^6.0.0",
    "postcss-preset-env": "^5.3.0",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
